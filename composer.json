{
    "name": "markup/addressing-bundle",
    "description": "Integration of markup/addressing with Symfony2.",
    "keywords": ["addresses", "postal", "addressing", "formatting"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Douglas Greenshields",
            "email": "douglas@usemarkup.com"
        },
        {
            "name": "Markup",
            "homepage": "http://www.usemarkup.com/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "markup/addressing": "~0.4.0",
        "symfony/framework-bundle": "^3.4|^4",
        "symfony/config": "^3.4|^4"
    },
    "require-dev": {
        "symfony/validator": "^3.4|^4",
        "phpunit/phpunit": "^7",
        "mockery/mockery": "^1",
        "symfony/console": "^3.4|^4",
        "phpstan/phpstan-shim": "^0.11"
    },
    "suggest": {
        "symfony/validator": "^3.4"
    },
    "autoload": {
        "psr-4": { "Markup\\AddressingBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
